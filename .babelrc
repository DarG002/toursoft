{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "node": "current",
        "esmodules": true
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-react-jsx",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-regenerator",
    "@babel/plugin-syntax-dynamic-import"]
}
